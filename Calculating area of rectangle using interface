interface shape{
    void area();
}
class Rectangle implements shape{
    int a;
    int b;
    public Rectangle(int a,int b){
        this.a=a;
        this.b=b;
    }
    public  void area(){
      int c=a*b;
    System.out.println("the area is:"+c);
    }
}
 public class show
 {
    public static void main(String[] args){
        Rectangle r1=new Rectangle(5,7);
      r1.area();
    }
}
